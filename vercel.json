{
	"name": "Filly Agioro",
	"public": true,
	"cleanUrls": true,
	"trailingSlash": false,
	"headers": [ 
    {
      "source": "/(.*)",
      "headers" : [
				{
					"key": "Content-Security-Policy",
					"value": "default-src https:"
				},
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        }
      ]
    }
	],
  "routes": [
    { "handle": "filesystem" },
		{ "src": "/(.*)", "status": 404, "dest": "/index.html" }
  ]
}
