{
	"name": "Filly Agioro",
	"version": 1,
	"public": true,
	"cleanUrls": true,
	"trailingSlash": false,
	"headers": [ 
		{
			"key": "Content-Security-Policy",
			"value": "default-src https:; frame-ancestors 'none'"
		},
		{
			"key": "X-Frame-Options",
			"value": "DENY"
		},
		{
			"key": "X-XSS-Protection",
			"value": "1; mode=block"
		},
		{
			"key": "X-Content-Type-Options",
			"value": "nosniff"
		}
	],
  "routes": [
    { "handle": "filesystem" },
		{ "src": "/(.*)", "status": 404, "dest": "/index.html" }
  ]
}
